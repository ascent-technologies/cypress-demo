{
  "name": "cypress-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "npx prettier --write 'src/**/*.{js,jsx,json}'",
    "test:unit": "npx react-scripts test --watchAll=false --all --ci --coverage ",
    "test:unit:watch": "npx react-scripts test --verbose ",
    "test:integration": "cypress run",
    "test:integration:live": "cypress open",
    "test:format": "npx prettier --check 'src/**/*.{js,jsx,json}'",
    "test:update": "npx react-scripts test --watchAll=false -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@material-ui/core": "4.3.2",
    "@material-ui/icons": "4.2.1",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.0",
    "classnames": "2.2.6",
    "cypress": "^3.8.1",
    "prop-types": "15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.3.0",
    "styled-components": "^4.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
